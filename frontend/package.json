{
  "name": "word-google-drive-plugin",
  "version": "1.0.0",
  "description": "在Word中访问和使用Google Drive文件的插件",
  "main": "src/taskpane.js",
  "scripts": {
    "start": "office-addin-debugging start manifest.xml",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "validate": "office-addin-manifest validate manifest.xml",
    "serve": "http-server -p 3000 -c-1 --ssl --cert cert.pem --key key.pem"
  },
  "dependencies": {
    "@fluentui/react": "^8.115.2",
    "@microsoft/office-js": "^1.1.0",
    "@microsoft/office-js-helpers": "^1.0.1",
    "core-js": "^3.34.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.13.11"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/preset-env": "^7.23.6",
    "@types/office-js": "^1.0.357",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.4",
    "http-server": "^14.1.1",
    "office-addin-debugging": "^5.0.8",
    "office-addin-dev-certs": "^1.11.3",
    "office-addin-manifest": "^1.12.2",
    "source-map-loader": "^4.0.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browserslist": [
    "ie 11",
    "last 3 versions"
  ]
}